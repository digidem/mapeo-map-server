{
  "name": "@mapeo/mapserver",
  "version": "1.0.0",
  "description": "Serve raster & vector map tiles along with style resources for GL maps",
  "main": "lib/server.js",
  "engines": {
    "node": "12.18.3"
  },
  "scripts": {
    "dev": "ts-node-dev --files --transpile-only --respawn --no-notify src/server.ts | pino-pretty",
    "start": "npm run clean && npm run build && node dist/src/server.js",
    "build": "npm run clean && tsc -b",
    "clean": "tsc -b --clean",
    "test": "TS_NODE_FILES=true tap --test-ignore='^dist'",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix && tsc --noEmit"
  },
  "author": "Digital Democracy",
  "license": "MIT",
  "dependencies": {
    "@mapbox/mapbox-gl-style-spec": "^13.23.0",
    "@mapbox/mbtiles": "^0.12.1",
    "@mapbox/tilebelt": "^1.0.2",
    "@mapbox/tilelive": "^6.1.0",
    "@mapbox/tiletype": "^0.3.1",
    "@mapbox/whoots-js": "^3.1.0",
    "@naturalatlas/mapbox-gl-native": "^1.0.2",
    "@sinclair/typebox": "^0.20.6",
    "@types/is-url": "^1.2.30",
    "@types/json-schema": "^7.0.9",
    "@types/lodash": "^4.14.177",
    "@types/mkdirp": "^1.0.2",
    "@types/mutexify": "^1.2.0",
    "@types/subleveldown": "^4.1.1",
    "@types/tilebelt": "^1.0.3",
    "ajv": "^8.8.1",
    "base32.js": "^0.1.0",
    "fastify": "^3.24.0",
    "fastify-error": "^0.2.0",
    "fastify-leveldb": "^3.1.1",
    "fastify-oas": "^3.0.8",
    "fastify-plugin": "^3.0.0",
    "fastify-swagger": "^4.12.6",
    "get-stream": "^6.0.1",
    "got": "^11.8.3",
    "hyperdrive": "^10.21.0",
    "install": "^0.13.0",
    "is-url": "^1.2.4",
    "level": "^6.0.1",
    "level-ts": "^2.1.0",
    "lodash": "^4.17.21",
    "make-promises-safe": "^5.1.0",
    "mkdirp": "^1.0.4",
    "mutexify": "^1.3.1",
    "promise.any": "^2.0.2",
    "quick-lru": "^5.1.1",
    "subleveldown": "^5.0.1",
    "tap": "^14.10.8"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.9",
    "@types/level": "^6.0.0",
    "@types/levelup": "^4.3.3",
    "@types/node": "^14.14.5",
    "@types/tap": "^14.10.1",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "fastify-etag": "^1.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.4.1",
    "tmp": "^0.2.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digidem/mapeo-mapserver.git"
  },
  "bugs": {
    "url": "https://github.com/digidem/mapeo-mapserver/issues"
  },
  "homepage": "https://github.com/digidem/mapeo-mapserver#readme"
}
