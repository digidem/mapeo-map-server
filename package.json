{
  "name": "@mapeo/map-server",
  "version": "1.0.0-alpha.13",
  "description": "Serve raster & vector map tiles along with style resources for GL maps",
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "engines": {
    "node": ">=16.17.1"
  },
  "scripts": {
    "dev": "ts-node-dev --files --transpile-only --respawn --no-notify ./server.ts | pino-pretty",
    "build": "npm run clean && node scripts/generate-sdf.js && tsc -b",
    "clean": "rm -rf ./dist ./sdf",
    "pretest": "npm run build",
    "test": "c8 node_modules/.bin/tape \"test/**/*.test.js\" | tap-arc",
    "posttest": "c8 report --reporter=text-lcov > ./coverage/lcov.info",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --fix && tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "prisma migrate dev",
    "prepare": "npm run build",
    "glyphs:generate": "node scripts/generate-sdf.js",
    "release": "standard-version"
  },
  "c8": {
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "author": "Digital Democracy",
  "license": "MIT",
  "files": [
    "dist",
    "prisma/migrations",
    "API.md",
    "sdf"
  ],
  "dependencies": {
    "@fastify/error": "^2.0.0",
    "@fastify/static": "^5.0.2",
    "@mapbox/sphericalmercator": "^1.2.0",
    "@mapbox/whoots-js": "^3.1.0",
    "@maplibre/maplibre-gl-style-spec": "^17.0.1",
    "@sinclair/typebox": "^0.24.51",
    "@types/readable-stream": "^2.3.15",
    "ajv": "^8.11.0",
    "base32.js": "^0.1.0",
    "fastify": "^3.29.0",
    "fastify-plugin": "^3.0.1",
    "got": "^11.8.5",
    "is-url": "^1.2.4",
    "make-promises-safe": "^5.1.0",
    "mem": "^8.1.1",
    "piscina": "^3.2.0",
    "quick-lru": "^5.1.1",
    "readable-stream": "^3.6.0",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "better-sqlite3": ">=9.3.0"
  },
  "devDependencies": {
    "@fastify/etag": "^3.0.0",
    "@mapbox/point-geometry": "^0.1.0",
    "@mermaid-js/mermaid-cli": "^9.0.3",
    "@tsconfig/node12": "^1.0.11",
    "@types/better-sqlite3": "^7.6.2",
    "@types/eventsource": "^1.1.10",
    "@types/is-url": "^1.2.30",
    "@types/mapbox__point-geometry": "^0.1.2",
    "@types/mapbox__sphericalmercator": "^1.1.5",
    "@types/node": "^18.11.9",
    "@types/tape": "^4.13.4",
    "@types/tilebelt": "^1.0.3",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "better-sqlite3": "^9.3.0",
    "c8": "^7.12.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eventsource": "^2.0.2",
    "fontnik": "^0.7.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.2.6",
    "mkdirp": "^1.0.4",
    "nock": "^13.2.9",
    "pino-pretty": "^7.6.1",
    "prettier": "^2.7.1",
    "prisma": "^3.15.2",
    "prisma-dbml-generator": "^0.9.1",
    "prisma-erd-generator": "^1.2.3",
    "standard-version": "^9.5.0",
    "tap-arc": "^0.3.5",
    "tape": "^5.7.4",
    "tmp": "^0.2.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digidem/mapeo-map-server.git"
  },
  "bugs": {
    "url": "https://github.com/digidem/mapeo-map-server/issues"
  },
  "homepage": "https://github.com/digidem/mapeo-map-server#readme"
}
